{
    "name": "eye-saver",
    "displayName": "Eye Saver",
    "description": "Help you to not forget make pause while work to save your eyes",
    "version": "0.0.1",
    "publisher": "kgchicobava",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.startEyeSaver"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.startEyeSaver",
                "title": "Save My Eyes"
            },
            {
                "command": "extension.setEyeSaverInterval",
                "title": "Set Eye Saver Interval"
            },
            {
                "command": "extension.stopEyeSaver",
                "title": "Stop Eye Saver"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
